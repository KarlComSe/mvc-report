{% extends 'kassabok/base.html.twig' %}

{% block title %}Välj, skapa eller anslut till en organisation{% endblock %}

{% block body %}
    {% form_theme form 'bootstrap_5_layout.html.twig' %}

    <h2>Välj befintlig organisation</h2>
    {% for organization in organizations %}
    {{ organization.name }}
    {{ dump(organization.users[0].email) }}
    {{ dump(organization.name) }}
    {% endfor %}
    <h2>Skapa ny organisation</h2>
    <div>
        {{ form(form) }}
    </div>
    <h2>Anslut till befintlig organisation</h2>
    <div>
        <p>Ett annat formulär</p>
    </div>
{% endblock %}
