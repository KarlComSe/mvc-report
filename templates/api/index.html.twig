{# templates/index.html.twig #}
{% extends 'base.html.twig' %}
{% block title %}API documentation{% endblock %}
{% block body %}
<main class="mt-3">
  <section>
    <h1>API documentation</h1>
    {% for key, value in api %}
      <article>
        <h2>{{ key }}</h2>
        <p>{{ value }}</p>
      </article>
    {% endfor %}
  </section>
</main>
{% endblock %}